#content-wrapper.d-flex.flex-column
  // Main Content
  #content
    include header
    // Begin Page Content
    .container
      // Page Heading
      h1.h4.mb-4.text-gray-800 #{titlePage}
      .addSales.mb-3
        .card.mb-5
          .card-body
            h4.card-header.mb-3 Choisir un produit
            ul.list-unstyled.d-flex.justify-content-between
              li
                a.d-flex.flex-column.mr-3.text-center(href="http://")
                  img(src="https://upload.wikimedia.org/wikipedia/commons/thumb/f/f9/Xbox_one_logo.svg/1024px-Xbox_one_logo.svg.png",width="80px", alt="", srcset="")
                  span Xbox
              li
                a.d-flex.flex-column.mr-3.text-center(href="http://")
                  img(src="https://img.maxisciences.com/article/international/l-acces-aux-achats-sur-le-playstation-network-est-impossible-depuis-le-debut-du-weekend_5d0c6dc6aaaa17f16ac0b2e98c0aec4a75dc512e.jpg",width="80px", alt="", srcset="")
                  span PSN
              li
                a.d-flex.flex-column.mr-3.text-center(href="http://")
                  img(src="https://banner2.kisspng.com/20180604/xiy/kisspng-steam-computer-icons-logo-video-game-valves-5b14dba6212cf3.1367866015280936061359.jpg",width="80px", alt="", srcset="")
                  span Steam
              li
                a.d-flex.flex-column.mr-3.text-center(href="http://")
                  img(src="https://www.erenumerique.fr/wp-content/uploads/2016/04/logo-google-play-store.jpg",width="80px", alt="", srcset="")
                  span GPLay
              li
                a.d-flex.flex-column.mr-3.text-center(href="http://")
                  img(src="https://upload.wikimedia.org/wikipedia/fr/thumb/0/09/Logo_App_Store_d%27Apple.png/220px-Logo_App_Store_d%27Apple.png",width="80px", alt="", srcset="")
                  span AppStore
              li
                a.d-flex.flex-column.mr-3.text-center(href="http://")
                  img(src="https://about.udemy.com/wp-content/uploads/2017/10/NewUlogo-large-1.png",width="80px", alt="", srcset="")
                  span Udemy
              li
                a.d-flex.flex-column.mr-3.text-center(href="http://")
                  img(src="data:image/png;base64,",width="80px", alt="", srcset="")
                  span CashU
        .card
          .card-body
            h4.card-header.mb-3 Détails de la commande
              small.ml-2.text-info produit selectionné (xbox)
            .card-body
              .row
                .input-group.col-6.mb-3
                  .input-group-prepend
                    span#basic-addon1.input-group-text $
                  input.form-control(type='number', placeholder='Indiquer le montant', aria-label='Username', aria-describedby='basic-addon1')
                .input-group.col-6.mb-3
                  .input-group-prepend
                    span#basic-addon1.input-group-text N°
                  input.form-control(type='text', placeholder='Numero de commande', aria-label='Username', aria-describedby='basic-addon1')
                .input-group.col-6.mb-3
                  .input-group-prepend
                    span#basic-addon1.input-group-text @
                  input.form-control(type='email', placeholder='Email Client', aria-label='email', aria-describedby='basic-addon1')
                .custom-file.col-6.mb-3
                  input#customFile.custom-file-input(type='file')
                  label.custom-file-label(for='customFile') Choisir la preuve de payement

            hr
            .card-body
              h5 choisir les coupons
              hr

